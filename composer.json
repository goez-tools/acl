{
    "name": "goez/acl",
    "description": "Simple role-based access control for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Jace Ju",
            "email": "jaceju@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0|^8.0",
        "codeclimate/php-test-reporter": "dev-master",
        "illuminate/support": ">=5.4 <=10"
    },
    "autoload": {
        "psr-4": {
            "Goez\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GoezTests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "keywords": [
        "rbac",
        "laravel"
    ]
}
